{
  "name": "rafikey-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "PORT=4433 serve dist",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "clear-pub": "rm -rfv public/assets/*.js public/assets/*.css",
    "sync-pub": "rsync -av --progress dist/ public/",
    "zip-pub": "rm -rfv rafikey.zip && zip -rv rafikey.zip public",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@vueuse/core": "^13.1.0",
    "dompurify": "^3.2.5",
    "highlight.js": "^11.8.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.541.0",
    "marked": "^13.0.2",
    "moment": "^2.30.1",
    "pinia": "^3.0.1",
    "preline": "^3.1.0",
    "uuid": "^11.1.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-sweetalert2": "^5.0.11"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.28",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "sweetalert2": "^11.14.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
